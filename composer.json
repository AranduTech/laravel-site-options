{
    "name": "arandu/laravel-site-options",
    "autoload": {
        "psr-4": {
            "Arandu\\LaravelSiteOptions\\": "src/"
        },
        "files": [
            "files/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Arandu\\LaravelSiteOptions\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bruno Polo",
            "email": "brunopolo@poli.ufrj.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.23",
        "illuminate/database": "^10.23"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arandu\\LaravelSiteOptions\\SiteOptionsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^8.15"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
